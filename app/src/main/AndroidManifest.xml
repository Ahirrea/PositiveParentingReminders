<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PositiveParentingReminders"
        >
        <activity
            android:name="com.positiveparenting.onboarding.OnboardingActivity"
            android:exported="true"
            android:theme="@style/Theme.PositiveParentingReminders">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.positiveparenting.onboarding.OnboardingStep2Activity"
            android:exported="false"
            android:theme="@style/Theme.PositiveParentingReminders" />
        <activity
            android:name="com.positiveparenting.onboarding.OnboardingStep3Activity"
            android:exported="false"
            android:theme="@style/Theme.PositiveParentingReminders" />
        <activity
            android:name="com.positiveparenting.onboarding.AccountCreationActivity"
            android:exported="false"
            android:theme="@style/Theme.PositiveParentingReminders" />
    </application>

</manifest>